<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="323.0" prefWidth="254.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.PrimaryController">
   <children>
      <Label text="Welcome" />
      <Button mnemonicParsing="false" onAction="#sendWarning" text="Send me a warning!" />
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Button fx:id="menuBtn" mnemonicParsing="false" text="Show Menu" />
            <TableView fx:id="menuTable" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn fx:id="menuColID" prefWidth="25.5999755859375" text="ID" />
                <TableColumn fx:id="menuColName" prefWidth="50.39996337890625" text="Name" />
                  <TableColumn fx:id="menuColPrice" prefWidth="40.79998779296875" text="Price" />
                  <TableColumn fx:id="menuColIng" prefWidth="96.79997863769532" text="Ingredients" />
              </columns>
            </TableView>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
