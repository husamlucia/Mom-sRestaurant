<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="197.0" prefWidth="859.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.CustomerController">
   <children>
      <TableView fx:id="brTable" layoutX="129.0" layoutY="51.0" prefHeight="94.0" prefWidth="351.0">
        <columns>
          <TableColumn fx:id="colBrId" prefWidth="75.0" text="BranchID" />
          <TableColumn fx:id="colOpenH" prefWidth="116.0" text="Open Hour" />
            <TableColumn fx:id="colCloseH" prefWidth="108.0" text="Closing Hour" />
        </columns>
      </TableView>
      <Button fx:id="shoMenuBtn" layoutX="10.0" layoutY="64.0" mnemonicParsing="false" onAction="#showMenu" prefHeight="25.0" prefWidth="103.0" text="Show Menu" />
      <Label layoutX="286.0" layoutY="14.0" prefHeight="25.0" prefWidth="159.0" text="MAIN PAGE FOR CLIENTS" />
      <Button fx:id="bookPlaceBtn" layoutX="198.0" layoutY="154.0" mnemonicParsing="false" onAction="#book" prefHeight="25.0" prefWidth="159.0" text="Book/Cancel Booking">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Button>
      <Button fx:id="complainBtn" layoutX="715.0" layoutY="154.0" mnemonicParsing="false" onAction="#complain" prefHeight="25.0" prefWidth="103.0" text="Complain">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Button>
      <Button layoutX="10.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" prefHeight="25.0" prefWidth="103.0" text="Back" />
      <TextArea layoutX="685.0" layoutY="42.0" prefHeight="94.0" prefWidth="133.0" />
      <Button layoutX="14.0" layoutY="154.0" mnemonicParsing="false" onAction="#createMaps" text="Create Maps with Tables" />

   </children>
</AnchorPane>
